<!-- begin css -->
<link href="/assets/isotope.css" rel="stylesheet"/>
<link href="/assets/lightbox.css" rel="stylesheet"/>
<!-- end css -->

<%= javascript_include_tag 'jquery.isotope', 'gallery.demo', 'sunny-detail', 'qrcode.min', 'lightbox-2.6.min', 'data-turbolinks-track' => "reload" %>


<div class="detail-container">

  <!-- Begin Content -->
  <div class="detail-content">

    <div class="detail-content-title">
      <h1>Nhà quận 1 Mạc Đĩnh Chi</h1>
      <h6>Mạc Đĩnh Chi, Quận 1, Hồ Chí Minh</h6>
    </div>

    <!-- Begin frame 3d -->
    <div class="detail-content-cover row">
      <div class="detail-icon-expand">
        <i class="fa fa-arrows-alt" aria-hidden="true"></i>
        <p>15 ảnh</p>
      </div>

      <a class="col-lg-4 col-sm-12 col-md-4 col-xs-12 no-padding" href="/assets/canho1.jpg" data-lightbox="gallery-group-1">
        <img src="<%= @coverUrl %>"/>
      </a>
      <div class="col-lg-8 col-sm-12 col-md-8 col-xs-12 no-padding" style="height: 400px; overflow: hidden">
        <iframe class="" style="width: 100%; height: 425px" src="<%= @matterUrl %>&amp;utm_source=hit-content&amp;play=1" frameborder="0" allowfullscreen allowvr></iframe>
      </div>
    </div>
    <!-- End frame 3d -->

    <!-- Begin Info -->
    <div class="detail-content-info container">
      <div class="row">
        <div class="detail-content-info-price col-lg-3 col-md-3 col-sm-6 col-xs-6">
          <h4>9.000.000.000 đ</h4>
          <h6>Giá căn hộ</h6>
        </div>

        <div class="detail-content-info-bedroom col-lg-2 col-md-2 col-sm-6 col-xs-6">
          <h4>3</h4>
          <h6>Số phòng ngủ</h6>
        </div>

        <div class="detail-content-info-bathroom col-lg-2 col-md-2 col-sm-6 col-xs-6">
          <h4>4</h4>
          <h6>Số phòng tắm</h6>
        </div>

        <div class="detail-content-info-size col-lg-3 col-md-3 col-sm-6 col-xs-6">
          <h4>200m2</h4>
          <h6>Kích thước hộ</h6>
        </div>

        <div class="detail-content-info-vr col-lg-2 col-md-2 col-sm-12 col-xs-12">
          <div id="qrcode"></div>
          <img class="detail-content-info-vr-icon" src="/assets/icon_vr.png"/>
        </div>
      </div>
    </div>
    <!-- End Info -->

    <!-- Begin Description -->
    <div class="detail-content-description container">
      <div class="row">
        <h4 class="col-lg-2 col-md-2 col-sm-4 col-xs-4">Mô tả</h4>
